<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsActive__c</fullName>
    <externalId>false</externalId>
    <formula>IF (
    AND ( TODAY() &gt;=  From__c, TODAY() &lt; To__c ),
    true, 
    false
)</formula>
